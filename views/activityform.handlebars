<div class="form-content">
  <div class="form-header">
    <h4 class="form-title">{title} Activity</h4>
    <!--<button type="button" class="btn-close" data-bs-dismiss="form" aria-label="Close"></button>-->
  </div>
  <div class="form-body">
    <form id="activity">

      <div class="dropdown">
        <h5>Select Duration</h5>
        <select class="form-select sleep-start form-select-lg mb-3" aria-label=".form-select-lg">
          <option selected>Duration</option>
          <option value="0000">15 MIN</option>
          <option value="0100">30 MIN</option>
          <option value="0200">45 MIN</option>
          <option value="0300">1 HR</option>
          <option value="0000">1 HR 15 MIN</option>
          <option value="0100">1 HR 30 MIN</option>
          <option value="0200">1 HR 45 MIN</option>
          <option value="0300">2 HR</option>
        </select>
      </div>
      <div class="activity-type">
        <h5>Select Duration</h5>

        <label for="lname">Last name:</label><br>
        <input type="text" id="lname" name="lname">

      </div>


    </form>
  </div>
  <div class="form-footer">
    <button type="button" class="btn btn-secondary" data-bs-dismiss="form">Close</button>
    <button type="button" class="btn btn-primary" href="..activityForm">Add Activity</button>
    <button type="submit" class="btn btn-primary">Save changes</button>
  </div>
</div>
</div>